{
    "eu.europa.ec.eudi.iban_sd_jwt_vc": {
        "format": "vc+sd-jwt",
        "vct": "eu.europa.ec.eudi.iban",
        "scope": "eu.europa.ec.eudi.iban",
        "cryptographic_binding_methods_supported": [
            "jwk",
            "cose_key"
        ],
        "credential_signing_alg_values_supported": [
            "ES256"
        ],
        "proof_types_supported": {
            "jwt": {
                "proof_signing_alg_values_supported": [
                    "ES256"
                ]
            },
            "cwt": {
                "proof_signing_alg_values_supported": [
                    "ES256"
                ],
                "proof_alg_values_supported": [
                    -7
                ],
                "proof_crv_values_supported": [
                    1
                ]
            }
        },
        "display": [
            {
                "name": "IBAN",
                "locale": "en"
            }
        ],
        "claims": {
            "sub": {
                "mandatory": true,
                "value_type": "string",
                "source": "user",
                "display": [
                    {
                        "name": "IBAN",
                        "locale": "en"
                    }
                ]
            },
            "id": {
                "mandatory": true,
                "source": "issuer",
                "display": [
                    {
                        "name": "Payment Means Document ID",
                        "locale": "en"
                    },
                    {
                        "name": "Dokumenta \"Maksāšanas veids\" identifikators",
                        "locale": "lv"
                    }
                ]
            },
            "scheme": {
                "mandatory": false,
                "value_type": "string",
                "source": "user",
                "display": [
                    {
                        "name": "Account Scheme",
                        "locale": "en"
                    },
                    {
                        "name": "Konta shēma",
                        "locale": "lv"
                    }
                ]
            },
            "psp": {
                "mandatory": false,
                "source": "issuer",
                "display": [
                    {
                        "name": "PSP Identifier",
                        "locale": "en"
                    },
                    {
                        "name": "Maksājumu servisa nodrošinātāja identifikators",
                        "locale": "lv"
                    }
                ]
            },
            "jti": {
                "mandatory": false,
                "source": "issuer",
                "display": [
                    {
                        "name": "Credential Identifier",
                        "locale": "en"
                    },
                    {
                        "name": "Elektroniskā dokumenta identifikators",
                        "locale": "lv"
                    }
                ]
            }
        }
    }
}